programa
{
	
	funcao inicio()
	{
		inteiro n1, n2
		
		escreva("Digite um numero: ")
		leia(n1)

		escreva("Digite um outro numero: ")
		leia(n2)

		escreva("A media e: ", media(n1, n2))

		
	}

	funcao inteiro media(inteiro num1, inteiro num2)
	{
		retorne (num1+num2)/2
	}
}
